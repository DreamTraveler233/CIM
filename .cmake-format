{
  "line_width": 80,
  "tab_size": 2,
  "max_subargs_per_line": 3,
  "separate_ctrl_name_with_space": false,
  "separate_fn_name_with_space": false,
  "dangle_parens": false,
  "dangle_align": "prefix",
  "min_prefix_chars": 4,
  "max_prefix_chars": 10,
  "max_lines_hwrap": 2,
  "line_ending": "unix",
  "command_case": "lower",
  "keyword_case": "upper",
  "always_wrap": [],
  "enable_sort": true,
  "autosort": true
}